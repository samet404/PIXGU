{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": "./",
    "paths": {
      "@/pixgu-peer": ["./src/pixgu-peer/pixguPeer.js"],
      "@/store/*": ["./src/store/*"],
      "@/middlewares": ["./src/middlewares/index.ts"],
      "@/helpers": ["./src/helpers/index.ts"],
      "@/helpers/room": ["./src/helpers/room/_index.ts"],
      "@/helpers/server": ["./src/helpers/server/index.ts"],
      "@/xata": ["./src/xata/"],
      "@/procedure": ["./src/server/api/procedure"],
      "@/schema/*": ["./src/server/db/sqlDb/schema/*"],
      "@/env/*": ["./src/env/*"],
      "@/db/*": ["./src/server/db/*"],
      "@/sqlDb": ["./src/server/db/sqlDb"],
      "@/sqlDb/*": ["./src/server/db/sqlDb/*"],
      "@/geoIP": ["./src/server/db/geoIP"],
      "@/redis/*": ["./src/server/db/redis/*"],
      "@/context/*": ["./src/context/*"],
      "@/scripts/*": ["./src/scripts/*"],
      "@/redis": ["./src/server/db/redis/"],
      "@/auth/*": ["./src/auth/*"],
      "@/api/*": ["./src/app/api/*"],
      "@/trpc/*": ["./src/trpc/*"],
      "@/components/*": ["./src/components/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/hooks": ["./src/hooks/_index.ts"],
      "@/server/*": ["./src/server/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/utils/server": ["./src/utils/server/_index.ts"],
      "@/utils/room": ["./src/utils/room/_index.ts"],
      "@/utils": ["./src/utils/_index.ts"],
      "@/styles/*": ["./src/styles/*"],
      "@/atoms": ["./src/atoms"],
      "@/zustand/*": ["./src/zustand/*"],
      "@/zod/*": ["./src/zod/*"],
      "@/types/*": ["./src/types/*"],
      "@/types": ["./src/types/_index.ts"],
      "@/app/*": ["./src/app/*"],
      "@/public/*": ["./public/*"],
      "@/svg/*": ["public/image/svg/*"],
      "@/png/*": ["public/image/png/*"],
      "@/jpg/*": ["public/image/jpg/*"],
      "@/gif/*": ["public/image/gif/*"],
      "@/sound/*": ["public/sound/*"],
      "@/fonts/*": ["public/fonts/*"],
      "@/lib/*": ["./src/lib*"],
      "@/actions/*": ["./src/actions/*"],
      "@/constants": ["./src/constants/index.ts"],
      "@/constants/server": ["./src/constants/server/index.ts"],
      "@/workers": ["./src/workers/index.ts"]
    },
    "plugins": [{ "name": "next" }]
  },
  "include": [
    ".eslintrc.cjs",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.js",
    "./.trash",
    "./src/components/Lines.tsx"
  ]
}
